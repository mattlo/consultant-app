{
  "presets": [
    "es2015",
    "react"
  ],
  "ignore": [
    "node_modules/**/*.js",
    "dist/**/*",
    "dist/**/*.js",
    "assets/**/*.js"
  ],
  "plugins": [
    "syntax-class-properties",
    "syntax-object-rest-spread",
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "webpack-start": {
      "plugins": [
        [
          "react-transform", {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            } ]
        } ]
      ] }
  }
}